{
  "name": "example-placeholders",
  "description": "Fills placeholder values",
  "private": true,
  "scripts": {
    "cy:run": "../node_modules/.bin/cypress run",
    "start": "../node_modules/.bin/parcel serve src/index.html",
    "test": "../node_modules/.bin/start-test 1234 cy:run",
